{% extends 'base.html.twig' %}

{% block title pin.title %}

{% block body %}
    <article>
        <h1>{{ pin.title }}</h1>
        <p>{{ pin.description }}</p>
    </article>

    <a href="{{ path('pins.edit', {id: pin.id}) }}">Edit pin</a> |
    <a href="{{ path('home') }}">Go back</a>
{% endblock %}